
info.app.name=My Super Cool App
info.app.description=A crazy and fun app, yoohoo!
info.app.version=1.0.0


# Use wild-card "*" to expose all end-points.
# Can also expose individual end-points with a comma-delimited list.
#
management.endpoints.web.exposure.include=*

# Exclude individual end-points with a comma-delimited list.
# management.endpoints.web.exposure.exclude=health,info 

management.info.env.enabled=true

#
# Define my crazy properties
#
coach.name=Mickey Mouse
team.name=The Mouse Club

#
# Change Spring Boot embedded server port
#
server.port=7070

#
# Set the context path of the application
#
server.servlet.context-path=/mycoolapp

